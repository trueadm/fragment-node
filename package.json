  "homepage": "https://github.com/trueadm/fragment-node#readme",
  "devDependencies": {
    "bdd-lazy-var": "^1.2.0",
    "buble": "^0.14.2",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-rollup-plugin": "^0.2.4",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
